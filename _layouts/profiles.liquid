            {% endif %}
          </div>
        {% endif %}
      {% endfor %}
      <br>

      <br>
      <h2>Previous Graduate Students</h2>
      <hr>
      <div class="grid-wrapper">
        <div class="grid">
          {% for profile in page.profiles %}
            {% if profile.role == 'PreviousGrad' %}
              <div class="grid-sizer"></div>
              <div class="grid-item" style="width:30%; margin-bottom:10px;">
                <div class="card">
                  <div class="card-body">
                      <h2 class="name">{{ profile.name }}</h2>
                      <hr>
                      <a href="{{ profile.portfolio_link }}" class="portfolio-link" style = "color:#17a2b8">Website</a>
                      <p class = "description" style = "margin-top:0px;">{{ profile.date }}</p>
                      <p class = "description" style = "margin-top:0px;">{{ profile.current }}</p>
                      
                      <p class = "description" style = "margin-top:0px;">{{ profile.lineBreak }}</p>
                    
                  </div>
                </div>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      </div>

      <h2>Previous Postdoctoral Researchers</h2>
      <hr>
      <div class="grid-wrapper">
        <div class="grid">
          {% for profile in page.profiles %}
            {% if profile.role == 'PreviousPostdoc' %}
              <div class="grid-sizer"></div>
              <div class="grid-item" style="width:30%; margin-bottom:10px;">
                <div class="card">
                  <div class="card-body">
                    <div class="header-section">
                      <h2 class="name">{{ profile.name }}</h2>
                      <p class="date" style = "margin-top:0px">{{ profile.date }}</p>
                    </div>
                    <hr>
                        {% if profile.portfolio_link %}
                          <a href="{{ profile.portfolio_link }}" class="portfolio-link" style = "color:#17a2b8;margin-top:0px">Website</a>
                        {%else%}
                          <a class="portfolio-link"></a>
                        {% endif %}
                        <p class="description" style = "margin-top:0px">{{ profile.description }}</p>
                        <p class="description" style = "margin-top:0px">{{ profile.lineBreak }}</p>
                  </div>
                </div>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      </div>

      <h2>Previous Student Visitors</h2>
      <br>
      <div class="grid-wrapper">
        <div class="grid">
          {% for profile in page.profiles %}
            {% if profile.role == 'PreviousVisitingStudent' %}
              <div class="grid-sizer"></div>
              <div class="grid-item" style="width:30%; margin-bottom:10px;">
                <div class="card">
                  <div class="card-body">
                      <h2 class="name">{{ profile.name }}</h2>
                      <p class="date" style = "margin-top:0px">{{ profile.date }}</p>
                      <hr>
                      {% if profile.portfolio_link %}
                      <a href="{{ profile.portfolio_link }}" class="portfolio-link" style = "color:#17a2b8; margin-top:0px;">Website</a>
                      {% endif %}
                      <p class="description" style = "margin-top:0px">{{ profile.description }}</p>
                      <p class="description" style = "margin-top:0px">{{ profile.university }}</p>
                      <p class="description" style = "margin-top:0px">{{ profile.lineBreak }}</p>
                      
                  </div>
                </div>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      </div>

      <br>
      <h2>Previous Faculty Visitors</h2>
      <br>
      <div class="grid-wrapper">
        <div class="grid">
          {% for profile in page.profiles %}
            {% if profile.role == 'PreviousVisitingFaculty' %}
              <div class="grid-sizer"></div>
              <div class="grid-item" style="width:30%; margin-bottom:10px;">
